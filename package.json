{
  "name": "atlas",
  "version": "1.0.0",
  "description": "",
  "main": "./out/extension.js",
  "scripts": {
    "watch": "tsc -watch -p ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.7.14",
    "@types/vscode": "^1.71.0",
    "typescript": "^4.8.2"
  },
  "engines": {
    "vscode": "^1.22.0"
  },
  "activationEvents": [
    "workspaceContains:**/*.atl",
    "onCommand:ATLAS.syncScripts",
    "onCommand:ATLAS.fillScripts"
  ],
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "plaintext",
        "path": "./snippets.json"
      },
      {
        "language": "yaml",
        "path": "./snippets.json"
      }
    ],
    "commands": [
      {
        "command": "ATLAS.syncScripts",
        "category": "ATLAS",
        "title": "Sync the scripts section"
      },
      {
        "command": "ATLAS.fillScripts",
        "category": "ATLAS",
        "title": "Fill the scripts section"
      }
    ]
  },
  "capabilities": {
    "completionProvider": {
      "resolveProvider": "true",
      "triggerCharacters": [
        ".",
        "$",
        ":"
      ]
    }
  },
  "dependencies": {
    "js-yaml": "^4.1.0"
  }
}
